{
  "name": "ForceCuts",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "The SFDC shortcut keys you wish you had.",
  "homepage_url": "http://www.github.com/patrickdepuydt",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        
        "*://*.na17.salesforce.com/*", "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}